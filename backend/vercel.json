{
  "version": 2,
  "rootDirectory": "backend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/dist/index.js" },
    { "src": "/(.*)", "dest": "/dist/index.js" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}